{
  "name": "simple-webgpu",
  "version": "0.10.0",
  "description": "WebGPU is a fast functional WebGPU framework.",

  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "TZ=America/Los_Angeles mocha 'test/**/*-test.js' && eslint src test",
    "compile": "rollup -c -w",
    "run": "rollup -c -w && serve ",
    "postpublish": "git push && git push --tags && cd ../d3.github.com && git pull && cp ../d3/dist/d3.js d3.v${npm_package_version%%.*}.js && cp ../d3/dist/d3.min.js d3.v${npm_package_version%%.*}.min.js && git add d3.v${npm_package_version%%.*}.js d3.v${npm_package_version%%.*}.min.js && git commit -m \"d3 ${npm_package_version}\" && git push && cd -"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/my-lib.umd.js",
  "module": "./dist/my-lib.es.js",
  "exports": {
    ".": {
      "import": "./dist/my-lib.es.js",
      "require": "./dist/my-lib.umd.js"
    }
  },
  "devDependencies": {
    "d3": "^7.1.1",
    "regl": "^2.1.0",
    "typescript": "^4.4.4",
    "vite": "^2.6.13",
    "gl-matrix": "^3.4.3",
    "mocha": "9",
    "rollup": "3",
    "@rollup/plugin-json": "6",
    "@rollup/plugin-node-resolve": "15",
    "rollup-plugin-terser": "7"

  },
  "dependencies": {  },
  "keywords": [
    "webgpu",
    "stackgl",
    "regl",
    "gl",
    "graphics",
    "computer graphics",
    "opengl",
    "wgsl",
    "data",
    "flow",
    "reactive",
    "functional"
  ],"license": "MIT",
  "bugs": {
    "url": "https://github.com/adnanwahab/simple-webgpu/issues"
  },
  "homepage": "https://simple-webgpu.com"
}
